{
  "name": "survey-app",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server  --config webpack.dev.config.js",
    "build-dev": "cross-env NODE_ENV=development webpack --config webpack.dev.config.js",
    "build:production": "npm run build-prod",
    "build-prod": "cross-env NODE_ENV=production webpack --display errors-only -p --config webpack.prod.config.js",
    "start-prod": "node server.js",
    "postinstall": "if-env NODE_ENV=production && npm run build-prod",
    "deploy": "git push origin master && git push heroku master",
    "deploy-live": "git push efex && git push atl",
    "heroku-redeploy": "git commit --allow-empty -m \"Trigger empty commit\" && git push heroku master"
  },
  "homepage": "https://ekz-survey.herokuapp.com/",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "bootstrap": "^4.5.3",
    "core-js": "^2.6.11",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "history": "4.10.1",
    "if-env": "^1.0.4",
    "install": "^0.13.0",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-apexcharts": "^1.3.7",
    "react-detect-offline": "^2.4.0",
    "react-dom": "^16.12.0",
    "react-paystack": "^3.0.3",
    "react-perfect-scrollbar": "^1.5.8",
    "reactstrap": "^8.8.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
